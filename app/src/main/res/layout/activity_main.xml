<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TableLayout
        android:id="@+id/tableLayout"
        android:layout_width="211sp"
        android:layout_height="211sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.1">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <EditText
                android:id="@+id/box1"
                android:layout_width="70sp"
                android:layout_height="70sp"
                android:autofillHints=""
                android:hint="@string/enter1"
                android:inputType="numberSigned" />

            <EditText
                android:id="@+id/box2"
                android:layout_width="70sp"
                android:layout_height="70sp"
                android:autofillHints=""
                android:hint="@string/enter2"
                android:inputType="numberSigned" />

            <EditText
                android:id="@+id/box3"
                android:layout_width="70sp"
                android:layout_height="70sp"
                android:autofillHints=""
                android:hint="@string/enter3"
                android:inputType="numberSigned" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <EditText
                android:id="@+id/box4"
                android:layout_width="70sp"
                android:layout_height="70sp"
                android:autofillHints=""
                android:hint="@string/enter4"
                android:inputType="numberSigned" />

            <EditText
                android:id="@+id/box5"
                android:layout_width="70sp"
                android:layout_height="70sp"
                android:autofillHints=""
                android:hint="@string/enter5"
                android:inputType="numberSigned" />

            <EditText
                android:id="@+id/box6"
                android:layout_width="70sp"
                android:layout_height="70sp"
                android:autofillHints=""
                android:hint="@string/enter6"
                android:inputType="numberSigned" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <EditText
                android:id="@+id/box7"
                android:layout_width="70sp"
                android:layout_height="70sp"
                android:autofillHints=""
                android:hint="@string/enter7"
                android:inputType="numberSigned" />

            <EditText
                android:id="@+id/box8"
                android:layout_width="70sp"
                android:layout_height="70sp"
                android:autofillHints=""
                android:hint="@string/enter8"
                android:inputType="numberSigned" />

            <EditText
                android:id="@+id/box9"
                android:layout_width="70sp"
                android:layout_height="70sp"
                android:autofillHints=""
                android:hint="@string/enter9"
                android:inputType="numberSigned" />
        </TableRow>

    </TableLayout>

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="Button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="183dp"
        android:layout_height="133dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button"
        app:layout_constraintVertical_bias="0.396" />

</androidx.constraintlayout.widget.ConstraintLayout>